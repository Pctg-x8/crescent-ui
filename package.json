{
  "name": "lunarlight",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev",
    "dev:ws": "dotenv -- tsx watch src/rpc/streamingServer.ts --tsconfig tsconfig.json",
    "dev": "run-p dev:*",
    "build": "next build",
    "build:ws": "esbuild src/rpc/streamingServer.ts --bundle --outdir=dist --minify --platform=node",
    "start": "next start",
    "lint": "next lint",
    "db:migrate-dev": "prisma migrate dev",
    "db:migrate-prod": "prisma migrate deploy",
    "db:generate-client": "prisma generate",
    "test": "jest",
    "typecheck": "tsc --pretty --noEmit",
    "fmtcheck": "prettier --check .",
    "prepare": "node scripts/dev-only.js && husky install || exit 0",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@babel/plugin-transform-typescript": "^7.22.5",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@linaria/babel-preset": "^4.4.5",
    "@linaria/core": "^4.2.10",
    "@linaria/react": "^4.3.8",
    "@prisma/client": "^4.11.0",
    "@tanstack/react-query": "^4.28.0",
    "@trpc/client": "^10.28.0",
    "@trpc/next": "^10.28.0",
    "@trpc/react-query": "^10.28.0",
    "@trpc/server": "^10.28.0",
    "@types/cookie": "^0.5.1",
    "@types/node": "18.14.0",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@types/ws": "^8.5.4",
    "classnames": "^2.3.2",
    "cookie": "^0.5.0",
    "dayjs": "^1.11.7",
    "esbuild": "^0.17.19",
    "eslint": "8.34.0",
    "eslint-config-next": "13.4.4",
    "immutable": "^4.3.0",
    "next": "13.4.4",
    "next-with-linaria": "^0.5.0",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.7",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prisma": "^4.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.3",
    "sass": "^1.60.0",
    "scss": "^0.2.4",
    "swr": "^2.1.1",
    "typescript": "^5.1.3",
    "universal-url": "^2.0.0",
    "ws": "^8.13.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@linaria/webpack5-loader": "^4.1.17",
    "@storybook/addon-essentials": "7.0.20",
    "@storybook/addon-interactions": "7.0.20",
    "@storybook/addon-links": "7.0.20",
    "@storybook/blocks": "7.0.20",
    "@storybook/nextjs": "7.0.20",
    "@storybook/react": "7.0.20",
    "@storybook/testing-library": "0.0.14-next.2",
    "@types/jest": "^29.5.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "eslint-plugin-storybook": "^0.6.12",
    "fast-check": "^3.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "storybook": "7.0.20",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7"
  },
  "volta": {
    "node": "19.8.1"
  },
  "babel": {
    "presets": [
      "next/babel",
      "@linaria"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-typescript",
        {
          "allowDeclareFields": true
        }
      ]
    ]
  }
}
